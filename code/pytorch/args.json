{
  "device": "cpu",
  "num_workers": 0,
  "pin_memory": false,

  "training_data": "../data/BootEA_DBP_WD_100K/",
  "output": "../output/results/",
  "word2vec_path": "../data/wiki-news-300d-1M.vec",
  "dataset_division": "631/",

  "alignment_module": "swapping",

  "encoder_epochs": 100,
  "encoder_activ": "",
